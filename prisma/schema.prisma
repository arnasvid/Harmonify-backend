generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id       Int      @id @default(autoincrement()) @db.Integer
    name     String
    email    String   @unique
    username String   @unique
    password String
    createAt DateTime @default(now())
    updateAt DateTime @updatedAt
}
